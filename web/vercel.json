{
  "framework": "vite",
  "rewrites": [
    { "source": "/quiz", "destination": "/quiz.html" },
    { "source": "/((?!assets|icons|sw.js|manifest.json|quiz.html).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}